{
  "name": "@bitte-ai/react",
  "version": "0.6.6-popup-wallet.1",
  "description": "React app tools for Bitte Wallet",
  "scripts": {
    "watch": "tsc && tsc --watch & jest --watch --coverage",
    "lint": "eslint . --fix --ext ts --ext tsx",
    "build": "tsup --config tsup.config.ts --external fsevents",
    "prepare": "pnpm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "Bitte Team",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react": "latest",
    "@testing-library/user-event": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "tsup": "latest",
    "typescript": "latest",
     "rxjs": "latest"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "browser": {
    "fsevents": false
  },
  "dependencies": {
    "@bitte-ai/wallet": "0.1.0-add-branch-release-and-injected-wallet-d4d511e",
    "@mintbase-js/data": "latest",
    "@mintbase-js/sdk": "latest",
    "@near-wallet-selector/core": "latest",
    "@near-wallet-selector/here-wallet": "latest",
    "@near-wallet-selector/meteor-wallet": "latest",
    "@near-wallet-selector/modal-ui": "latest",
    "@near-wallet-selector/my-near-wallet": "latest",
    "buffer": "latest",
    "react": "latest",
    "react-dom": "latest"
  }
